	<!--Features 2 ####################-->
	<!--###############################-->

	<section id="features_2">
		<div class="container">
			<div class="row">
				
				<div class="col-sm-6 col-md-6 col-lg-6" id="features_2_content">
					<h2>Stay on top of your tasks</h2>
					<p>Taskie ensures that you and the people you connect with are kept in a constant loop about the tasks and its progress, with prompt feedback and status updates.
					</p>
					<div class="feature">
						<svg viewBox="0 0 32 32" class="feature_icon">
							<use xlink:href="#ribbon"></use></svg>
						<div>
							<h4>Push Notifications</h4>
							<p>Recieve quick and prompt updates about you tasks<br>via push and list notifications</p>
						</div>
					</div>
					
					<div class="feature">
						<svg viewBox="0 0 32 32" class="feature_icon">
							<use xlink:href="#rotate"></use></svg>
						<div>
							<h4>Share and Collaborate</h4>
							<p>Send tasks to not just one<br>but with many people at a time.</p>
						</div>
					</div>
					
				</div>
				
				
				<div class="col-sm-6 col-md-6  col-lg-6" >
					<img src="images/features2a.png" class="img-responsive image_front" id="features2b_image" alt="img">
				</div>

			</div>
			
		</div>
		
	</section>

